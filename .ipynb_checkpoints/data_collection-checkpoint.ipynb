    "import tweepy as tw\n",
    "from tweepy import OAuthHandler\n",
    "import json\n",
    "import csv\n",
    "import os.path\n",
    "\n",
    "# Load credentials from json file\n",
    "with open(DIR+\"twitter_credentials.json\", \"r\") as file:\n",
    "    creds = json.load(file)\n",
    "\n",
    "consumer_key = creds['CONSUMER_KEY']\n",
    "consumer_secret = creds['CONSUMER_SECRET']\n",
    "\n",
    "access_token = creds['ACCESS_TOKEN']\n",
    "access_token_secret = creds['ACCESS_SECRET']\n",
    "\n",
    "auth = tw.OAuthHandler(consumer_key, consumer_secret)\n",
    "auth.set_access_token(access_token, access_token_secret)\n",
    "\n",
    "api = tw.API(auth,wait_on_rate_limit=True,wait_on_rate_limit_notify=True)\n",
    "\n",
    "# Save each user data to csv file\n",
    "def save_to_csv(user_data,filename,fieldnames):\n",
    "    file_exists = os.path.isfile(filename)\n",
    "    with open(filename, 'a') as file:\n",
    "        writer = csv.DictWriter(file,fieldnames=fieldnames)\n",
    "        if not file_exists:\n",
    "            writer.writeheader()  # file doesn't exist yet, write a header\n",
    "        writer.writerow(user_data)\n",
    "        \n",
    "# Process User\n",
    "def process_user(user):\n",
    "    d = {}\n",
    "    d['id'] = user.id\n",
    "    d['location'] = user.location\n",
    "    d['followers_count'] = user.followers_count \n",
    "    d['favourites_count'] = user.favourites_count\n",
    "    d['friends_count'] = user.friends_count\n",
    "    d['statuses_count'] = user.statuses_count\n",
    "    d['last_status_id'] = user.status.id\n",
    "    d['last_status_date'] = user.status.created_at\n",
    "    d['last_status_retweet_count'] = user.status.retweet_count\n",
    "    d['last_status_favorite_count'] = user.status.favorite_count\n",
    "    return d\n",
    "\n",
    "count = 0\n",
    "count_deleted = 0\n",
    "deleted_account = []\n",
    "\n",
    "# Fieldnames for header of csv file\n",
    "fieldnames_user = ['id', 'location','followers_count','favourites_count',\n",
    "                      'friends_count','statuses_count','last_status_id',\n",
    "                      'last_status_date','last_status_retweet_count','last_status_favorite_count'\n",
    "                     ]\n",
    "fieldnames_deleted = ['id','deleted_account_id']\n",
    "# Iter on each user id in the ego_timelines database to query information via tweeter api\n",
    "for i in ego_timelines.drop_duplicates(subset=['id']).id:\n",
    "    try:\n",
    "        user = api.get_user(i)\n",
    "        user_data = process_user(user)\n",
    "        save_to_csv(user_data,'./data/recent_user_information.csv',fieldnames_user)\n",
    "    except:\n",
    "        d = {}\n",
    "        d['id'] = user.id\n",
    "        d['deleted_account_id'] = str(i)\n",
    "        save_to_csv(d,'./data/deleted_account.csv',fieldnames_deleted)\n",
    "        count_deleted += 1\n",
    "\n",
    "\n"
   ]
